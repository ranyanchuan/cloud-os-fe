		"build:premises": "npm run premises && npm run premises_en && npm run premises_tw && npm run premises_copy",
		"pre": "node scripts/formal.js LAN_TYPE=src environment=pre",
		"pre_en": "node scripts/formal.js LAN_TYPE=en environment=pre",
		"pre_tw": "node scripts/formal.js LAN_TYPE=tw environment=pre",
		"formal": "node scripts/formal.js LAN_TYPE=src environment=formal",
		"formal_en": "node scripts/formal.js LAN_TYPE=en environment=formal",
		"formal_tw": "node scripts/formal.js LAN_TYPE=tw environment=formal",
